{
  "name": "abc/job-worker-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony to process jobs from AbcJobServerBundle",
  "keywords": ["job", "asynchronous", "distributed", "queue", "broker"],
  "homepage": "http://aboutcoders.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Schulz",
      "email": "hannes.schulz@aboutcoders.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/framework-bundle": "^3.4|^4",
    "abc/job": "dev-route-api"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "symfony/browser-kit": "^3.4|^4",
    "symfony/yaml": "^3.4|^4",
    "enqueue/enqueue-bundle": "^0.9",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": { "Abc\\JobWorkerBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
